func rotate(_ matrix: inout [[Int]]) {
    let n = matrix.count
    
    for i in 0..<n/2 {
        for j in i..<n-1-i {
            let var1 = matrix[i][j]
            let var2 = matrix[j][n-1-i]
            let var3 = matrix[n-1-i][n-1-j]
            let var4 = matrix[n-1-j][i]
            
            matrix[i][j] = var4
            matrix[j][n-1-i] = var1
            matrix[n-1-i][n-1-j] = var2
            matrix[n-1-j][i] = var3
        }
    }
}
