    public int countBattleships(char[][] board) {
        int len1 = board.length;
        if(len1 == 0){
            return 0;
        }
        int len2 = board[0].length;
        int ans = 0;
        for (int i = 0; i < len1; i++){
            for(int j=0 ; j < len2; j++){
                if(board[i][j] == 'X' && (i == 0 || board[i-1][j] == '.') && (j == 0 || board[i][j-1] == '.')){
                    ans ++;
                }
            }
        }
        return ans;
    }
